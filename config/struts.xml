<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
          "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="account" extends="struts-default" namespace="/">
    	<action name="login" class="com.lw.file.action.AccountAction" method="login">
    		<result name="success">/WEB-INF/main/main.jsp</result>
    		<result name="loginFailed">/account/login.jsp</result>
    	</action>
    	<action name="fileupload" class="com.lw.file.action.FileAction" method="upload">
    		<result name="uploadSuccess">/WEB-INF/main/main.jsp</result>
    	</action>
    </package> 
</struts>
